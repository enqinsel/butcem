      "usesCleartextTraffic": true,
      "package": "com.inaftech.butcem",
      "versionCode": 10
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-sqlite"
    ],
    "extra": {
      "GEMINI_API_KEY": "YOUR_GEMINI_API_KEY_HERE",
      "eas": {
        "projectId": "76485f92-73ec-4b55-9e2e-720bedb996b1"
